    case 5: inv_add(reward, naturerune, calc(10 + random(11)));
    case 6: inv_add(reward, chaosrune, calc(10 + random(11)));
    case 7: inv_add(reward, firerune, calc(50 + random(51)));
    case 8: inv_add(reward, earthrune, calc(50 + random(51)));
    case 9: inv_add(reward, waterrune, calc(50 + random(51)));
