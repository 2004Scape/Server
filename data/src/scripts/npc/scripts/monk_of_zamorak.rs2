[ai_opplayer2,monk_of_zamorak_22]
if (%npc_action_delay > map_clock) return;
def_int $whichattack = random(4); // approximation by joe testing on osrs https://i.imgur.com/qXBNa40.png
if ($whichattack = 0) {
    ~npc_zap_attack(8,npc_param(attackrate));
} else {
    ~npc_default_attack;
}


