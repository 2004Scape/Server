[ai_queue3,necromancer]
gosub(npc_death);
if (npc_findhero = false) {
    return;
}

// Default drop from config.
obj_add(npc_coord, npc_param(death_drop), 1, ^lootdrop_duration);
def_int $random = random(20);

if ($random < 8) {
    obj_add(npc_coord, plainstaff, 1, ^lootdrop_duration);
} else if ($random < 12) {
    obj_add(npc_coord, zamorak_monk_bottom, 1, ^lootdrop_duration);
} else if ($random < 15) {
    obj_add(npc_coord, zamorak_monk_top, 1, ^lootdrop_duration);
} else if ($random < 16) {
    obj_add(npc_coord, staff_of_fire, 1, ^lootdrop_duration);
} else if ($random < 20) {
