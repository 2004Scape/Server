    %prayer_drain_effect = sub(%prayer_drain_effect, enum(component, int, prayer_drain_effects, last_com));
    %prayer1 = ^notactive;
    sound_synth(cancel_prayer, 0, 0);
    if (~prayers_are_active = false) {
        clearsofttimer(drain_prayer);
