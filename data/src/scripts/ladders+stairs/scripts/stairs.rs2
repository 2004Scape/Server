[oploc1,loc_1738]
def_coord $coord = loc_coord();

// lumbridge castle stairs level 0
if($coord = 0_50_50_4_7) {
    p_telejump(1_50_50_5_9);
} else if($coord = 0_40_50_12_40) { // Clock Tower - level 0
    p_telejump(1_40_50_11_41);
} else {
    mes("unhandled stair <~coord_tostring($coord)>");
}

