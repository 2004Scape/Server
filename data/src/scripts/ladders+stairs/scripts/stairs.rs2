[oploc1,loc_1740]
def_coord $coord = loc_coord();

// lumbridge castle stairs level 2
if($coord = 2_50_50_5_8) {
    p_telejump(1_50_50_5_9);
} else if($coord = 2_40_50_12_41) { // Clock Tower - top level
