[label,opheldu_make_gnome_food](namedobj $gnome_food, string $message)
if (last_useitem ! gnome_spice) {
    inv_del(inv, last_useitem, 1);
}
if (last_item ! gnome_spice) {
    inv_del(inv, last_item, 1);
}
inv_add(inv, $gnome_food, 1);
mes($message);

