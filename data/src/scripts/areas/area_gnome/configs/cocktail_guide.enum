val=3,1 shot vodka\n1 shot gin\nDwellberry juice
val=4,Juice 1 pineapple\nJuice 1 orange\nJuice 1 lemon
val=5,Juice 1 lemon\nJuice 1 orange\nJuice 2 pineapples
