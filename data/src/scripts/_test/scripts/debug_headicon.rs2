[debugproc,headicon_add]
~add_headicon(^headicon_skull);
~add_headicon(^headicon_prayer_protectfrommelee);

[debugproc,headicon_del_skull]
~del_headicon(^headicon_skull);

[debugproc,headicon_del_prayer]
~del_headicon(^headicon_prayer_protectfrommelee);

[debugproc,headicon_add_all]
~add_headicon(^headicon_skull);
~add_headicon(^headicon_multi);
~add_headicon(^headicon_hint);
~add_headicon(^headicon_prayer_protectfrommelee);
~add_headicon(^headicon_prayer_protectfromrange);
~add_headicon(^headicon_prayer_protectfrommagic);
~add_headicon(^headicon_duel);
~add_headicon(^headicon_hint2);

[debugproc,headicon_clear]
~clear_headicons;
