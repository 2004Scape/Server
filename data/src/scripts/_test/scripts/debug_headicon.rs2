[debugproc,headicon_add]
~headicon_add(^headicon_skull);
~headicon_add(^headicon_prayer_protectfrommelee);

[debugproc,headicon_del_skull]
~headicon_del(^headicon_skull);

[debugproc,headicon_del_prayer]
~headicon_del(^headicon_prayer_protectfrommelee);

[debugproc,headicon_add_all]
~headicon_add(^headicon_skull);
~headicon_add(^headicon_multi);
~headicon_add(^headicon_hint);
~headicon_add(^headicon_prayer_protectfrommelee);
~headicon_add(^headicon_prayer_protectfromrange);
~headicon_add(^headicon_prayer_protectfrommagic);
~headicon_add(^headicon_duel);
~headicon_add(^headicon_hint2);

[debugproc,headicon_clear]
~headicons_clear;

[debugproc,headicon_skull_exists]
if (~headicon_exists(^headicon_skull) = true) {
    mes("Skull is active.");
} else {
    mes("Skull is not active.");
}

[debugproc,headicon_prayer_exists]
if (~headicon_exists(^headicon_prayer_protectfrommelee) = true) {
    mes("Prayer melee is active.");
} else {
    mes("Prayer melee is not active.");
}
