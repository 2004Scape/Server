[debugproc,loc_change_dynamic]
if (p_finduid(uid) = true) {
    p_telejump(0_37_55_32_9);
    p_delay(0);
    loc_add(0_37_55_32_9, loc_1276, 0, centrepiece_straight, 10);
    p_delay(1);
