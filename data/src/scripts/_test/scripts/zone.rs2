[debugproc,map_anim](spotanim $spotanim, int $height, int $delay)
if ($spotanim = null) {
    mes("Usage: ::map_anim (spotanim) (height) (delay)");
    return;
}

if ($height = 0) {
    $height = 100;
}

map_anim(coord, $spotanim, $height, $delay);

[debugproc,map_projanim]
npc_findallzone(coord);
if (npc_findnext = true) {
    mes("Firing at npc");
    map_projanim_npc(coord, npc_uid, spotanim_18, 40, 36, 41, 60, 13, 15);
    return;
}

mes("Firing at coord");
map_projanim_coord(coord, movecoord(coord, 0, 0, 5), spotanim_18, 40, 36, 41, 60, 13, 15);
