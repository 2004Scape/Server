[debugproc,npc_telejump_walk]
def_coord $coord = movecoord(coord, 0, 0, 1);

def_npc $npc = npc_494;
npc_add($coord, $npc, 50);

p_delay(0);

npc_findallzone($coord);
while (npc_findnext = true) {
    if (npc_type = npc_494) {
        npc_telejump(movecoord($coord, 0, 0, 2));
    }
}

p_delay(0);

npc_findallzone(movecoord($coord, 0, 0, 2));
while (npc_findnext = true) {
    if (npc_type = npc_494) {
        npc_telejump(movecoord($coord, 0, 0, 0));
    }
}

p_delay(0);

npc_findallzone($coord);
while (npc_findnext = true) {
    if (npc_type = npc_494) {
        npc_telejump(movecoord($coord, 0, 0, 2));
    }
}

p_delay(0);

npc_findallzone(movecoord($coord, 0, 0, 2));
while (npc_findnext = true) {
    if (npc_type = npc_494) {
        npc_telejump(movecoord($coord, 0, 0, 0));
    }
}

