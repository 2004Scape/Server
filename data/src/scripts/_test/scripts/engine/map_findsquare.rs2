[debugproc,map_findsquare]
mes(~coord_tostring(map_findsquare(coord, 1, 2, 1)));
if (p_finduid(uid) = true) {
    p_telejump(map_findsquare(coord, 1, 2, 1));
}

[debugproc,imp](int $count)
def_int $i = 0;
def_coord $coord = coord;
while ($i < $count) {
    def_coord $new_coord = map_findsquare($coord, 0, 20, ^map_findsquare_none);
    mes("<tostring(sub(coordx($coord), coordx($new_coord)))>, <tostring(sub(coordz($coord), coordz($new_coord)))>");
    $i = add($i, 1);
}