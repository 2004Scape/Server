[debugproc,huntall_primary]
huntall(coord, 10, 0);
while (huntnext = true) {
    mes("Hello noob.");
}

[debugproc,huntall_secondary]
huntall(coord, 10, 0);
while (.huntnext = true) {
    .mes("Hello noob.");
}

[debugproc,npc_findall](npc $type)
npc_findall(coord, $type, 10, 1);
while (npc_findnext = true) {
    npc_say("WEEE!!!");
}
