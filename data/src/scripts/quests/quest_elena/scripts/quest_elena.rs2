[proc,quest_elena_set_progress](int $stage)
%elena_progress = $stage;
~send_quest_progress(quest_journal:elena, %elena_progress, ^elena_complete);

[queue,quest_elena_complete]
~quest_elena_set_progress(^elena_complete);
~send_quest_complete(quest_journal:elena, gasmask, 220, 1, "You have completed the\\nPlague City Quest!");
