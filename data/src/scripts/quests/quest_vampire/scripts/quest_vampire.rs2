[proc,quest_vampire_set_progress](int $stage)
%vampire_progress = $stage;
~send_quest_progress(quest_journal:vampire, %vampire_progress, ^vampire_complete);

[queue,quest_vampire_complete]
givexp(attack, 48250);
~quest_vampire_set_progress(^vampire_complete);
~send_quest_complete(quest_journal:vampire, stake, 220, 3, "You have completed the\\nVampire Slayer Quest!");
