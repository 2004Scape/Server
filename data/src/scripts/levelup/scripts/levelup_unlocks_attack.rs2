[label,levelup_attack]
//Attack level up unlock messages
switch_int(stat_base(attack)) {
    case 5: ~objboxt(steel_longsword,"You can now wield @dbl@Steel Weaponry@bla@!");
    case 10: ~objboxt(black_longsword,"You can now wield @dbl@Black Weaponry@bla@!");
    case 20: ~objboxt(mithril_longsword,"You can now wield @dbl@Mithril Weaponry@bla@!");
    case 30: ~objboxt(adamant_longsword,"You can now wield @dbl@Adamant Weaponry@bla@!");
    case 40: ~objboxt(rune_longsword,"You can now wield @dbl@Rune Weaponry@bla@!");
    case 60: ~objboxt(dragon_longsword,"Members can now wield @dbl@Dragon Weaponry@bla@!");
}