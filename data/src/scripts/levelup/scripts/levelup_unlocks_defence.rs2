[label,levelup_defence]
//Defence level up unlock messgaes
switch_int(stat_base(defence)) {
    case 5 : ~objbox(steel_platebody,"You can now wear @dbl@Steel Armour!", 250, 0, 0);
    case 10 : ~objbox(black_platebody,"You can now wear @dbl@Black Armour!", 250, 0, 0);
    case 20 : ~objbox(mithril_platebody,"You can now wear @dbl@Mithril Armour!", 250, 0, 0);
    case 30 : ~objbox(adamant_platebody,"You can now wear @dbl@Adamant Armour!", 250, 0, 0);
    case 40 : ~objbox(rune_platebody,"You can now wear @dbl@Rune Armour!", 250, 0, 0);
    case 60 : ~objbox(dragon_med_helm,"Members can now wear @dbl@Dragon Armour!", 250, 0, ^objbox_height); //imgur.com/bpoMZgD
}