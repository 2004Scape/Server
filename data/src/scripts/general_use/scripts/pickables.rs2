// Pickable Potatoes
[oploc2,loc_312]
p_arrivedelay;
if(inv_freespace(inv) = 0) {
    mes("You don't have room for this potato.");
    p_tele(loc_coord);
    p_delay(0);
} else {
    anim(bury, 0);
    p_tele(loc_coord);
    p_delay(0);
    sound_synth(pick, 0, 0);
    loc_del(50);
    inv_add(inv, potato, 1);
    mes("You pick a potato.");
}

