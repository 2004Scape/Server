// accurate
// rapid
// longrange

[proc,attack_bow_swing](obj $obj, int $style)(seq, synth)
switch_int ($style) {
    case ^style_bow_accurate, ^style_bow_rapid, ^style_bow_longrange:
        return(oc_param($obj, rangeattack_anim), oc_param($obj, rangeattack_sound));
}
error("style of <tostring($style)> not defined in switch for attack_bow_swing");
