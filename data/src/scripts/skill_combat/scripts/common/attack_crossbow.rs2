// accurate
// rapid
// longrange

[proc,attack_crossbow_swing](obj $obj, int $style)(seq, synth)
switch_int ($style) {
    case ^style_crossbow_accurate, ^style_crossbow_rapid, ^style_crossbow_longrange:
        return(oc_param($obj, rangeattack_anim), oc_param($obj, rangeattack_sound));
}
error("style of <tostring($style)> not defined in switch for attack_crossbow_swing");
