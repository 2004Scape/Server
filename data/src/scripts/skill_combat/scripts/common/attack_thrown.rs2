// accurate
// rapid
// longrange

[proc,attack_thrown_swing](obj $obj, int $style)(seq, synth)
switch_int ($style) {
    case ^style_thrown_accurate, ^style_thrown_rapid, ^style_thrown_longrange:
        return(oc_param($obj, rangeattack_anim), oc_param($obj, rangeattack_sound));
}
error("style of <tostring($style)> not defined in switch for attack_thrown_swing");
