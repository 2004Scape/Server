            <tr><td valign=center><a href="/client?world=<%= regionWorlds[j].id %>&detail=<%= detail %>&method=<%= method %>" oncontextmenu=return(false) target="_parent" class=c>World <%= regionWorlds[j].id %></a></td><td align=right><%= regionWorlds[j].players.length %> player<%= regionWorlds[j].players.length != 1 ? 's' : '' %></td></tr>
          <% } %>
          <% if (i + 1 < freeRegions.length) { %>
            <tr><td colspan=2>&nbsp;</td></tr>
          <% } %>
      <% if (((i + 1) % 3) == 0) { %>
        </table>
        </td>
      <% } %>
      <% if (freeRegions.length % 3 != 0 && i == freeRegions.length - 1) { %>
        </table>
        </td>
      <% } %>
    <% } %>
  </tr>
</table>
