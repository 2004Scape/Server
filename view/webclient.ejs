<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=0.7, shrink-to-fit=no">
    <title>2004Scape Game</title>

    <style>
      body,
      td,
      p {
        font-family: Arial, Helvetica, sans-serif;
        font-size: 11px;
      }
      body {
        margin: 0px;
        overflow: auto;
      }
      .b {
        border-style: outset;
        border-width: 3pt;
        border-color: #373737;
      }
      .e {
        border: 2px solid #382418;
      }
      .c {
        text-decoration: none;
      }
      A.c:hover {
        text-decoration: underline;
      }
    </style>
  </head>
  <body bgcolor="black" text="white" link="#90c040" alink="#90c040" vlink="#90c040" style="margin: 2px 0 0 0 0;" marginwidth="0" marginheight="0" topmargin="2" leftmargin="0">
    <center>
      <table cellpadding="1" cellspacing="0">
        <tr>
          <td align="<%= world.members ? 'left' : 'center' %>">
            <table <%- world.members ? '' : 'width="785"' %> cellpadding="1" cellspacing="0">
              <tr>
                <td align="center">
                  <a href="/title" id="menulink">
                    <% if (world.members) { %>
                    <img src="/img/title/mm2_rs2.jpg" height="65" border="0">
                    <% } else { %>
                    <img src="/img/jagexlogo_black.jpg" border="0"><br>
                    <font color="white">Return to Main Menu</font>
                    <% } %>
                  </a>
                </td>
                <% if (!world.members) { %>
                <td align="center">
                  <iframe src="/banner?size=469" width="468" height="60" marginwidth="0" marginheight="0" frameborder="0" scrolling="no"></iframe>
                </td>
                <td align="right">
                  <a href="/" target="newwin">
                    <img src="/img/gamewin/realbanner.gif" border="0">
                  </a>
                </td>
                <% } else { %>
                <td align="left">
                  <a href="/title" id="menulink2">
                    <font color="white">Return to<br>Main Menu</font>
                  </a>
                </td>
                <% } %>
              </tr>
            </table>
          </td>
        </tr>
        <tr>
          <td colspan="<%= world.members ? 1 : 2%>" align="center">
            <iframe src='<%= world.address %>/client-inner?method=<%= method %>&world=<%= world.id %>&detail=<%= detail %>' frameborder="0" border="0" width="789" height="560" scrolling="no" allowfullscreen></iframe>
          </td>
        </tr>
      </table>
    </center>
  </body>
</html>
