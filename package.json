{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#3rdparty/*": "./src/3rdparty/*",
    "#jagex2/*": "./src/jagex2/*",
    "#lostcity/*": "./src/lostcity/*"
  },
  "scripts": {
    "build": "npm run client:pack && npm run server:build",
    "clean": "npm run client:clean && npm run server:clean",
    "client": "java -cp 225.2004-05-18.jar client 10 0 highmem members",
    "client:clean": "npm run ts-loader src/lostcity/tools/client/clean.ts",
    "client:pack": "npm run ts-loader src/lostcity/tools/pack/client.ts",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:schema": "prisma migrate dev",
    "dev": "concurrently \"nodemon src/lostcity/app.ts\" \"npm run server:watch\"",
    "drops": "npm run ts-loader src/lostcity/tools/server/drops.ts",
    "friend": "npm run ts-loader src/lostcity/friend.ts",
    "lint": "eslint . --ext .ts --ext .js",
    "lint:staged": "eslint --ext .ts --ext .js",
    "login": "npm run ts-loader src/lostcity/login.ts",
    "maintenance": "npm run ts-loader src/lostcity/maintenance.ts",
    "precommit": "prettier . --write && eslint . --ext .ts --ext .js --fix",
    "prepare": "husky",
    "script:compile": "java -jar RuneScriptCompiler.jar",
    "script:run": "npm run ts-loader src/lostcity/tools/server/runscript.ts",
    "server:build": "npm run server:pack && npm run script:compile",
    "server:clean": "npm run ts-loader src/lostcity/tools/server/clean.ts",
    "server:pack": "npm run ts-loader src/lostcity/tools/pack/server.ts",
    "server:watch": "watch \"npm run server:build\" ./data/src/scripts ./src/lostcity/tools",
    "start": "npm run ts-loader src/lostcity/app.ts",
    "test": "vitest run",
    "todo": "leasot src/ --skip-unsupported --exit-nicely",
    "ts-loader": "node --enable-source-maps --no-warnings --loader ts-node/esm",
    "web": "npm run ts-loader src/lostcity/web.ts"
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "*.{js,ts}": [
      "npm run lint:staged",
      "leasot --skip-unsupported --exit-nicely"
    ]
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^6.12.0",
    "@fastify/view": "^8.2.0",
    "@jimp/png": "^0.22.10",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.25.2",
    "jimp": "^0.22.10",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.5",
    "node-forge": "^1.3.1",
    "watch": "^1.0.2",
    "ws": "^8.15.1"
  },
  "devDependencies": {
    "@2004scape/rsmod-pathfinder": "^4.2.9",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@swc/core": "1.3.101",
    "@swc/helpers": "^0.5.3",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.10.5",
    "@types/node-forge": "^1.3.10",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.0.11",
    "leasot": "^13.3.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.2",
    "prettier": "3.2.4",
    "prisma": "^5.7.1",
    "prisma-kysely": "^1.7.1",
    "sort-package-json": "^2.10.0",
    "tinybench": "^2.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.5.0"
  },
  "prisma": {
    "seed": "npm run ts-loader src/lostcity/db/seed.ts"
  }
}
