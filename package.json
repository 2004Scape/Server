{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#jagex2/*": "./src/jagex2/*",
    "#lostcity/*": "./src/lostcity/*",
    "#rsmod/*": "./src/rsmod/*"
  },
  "scripts": {
    "start": "nodemon src/lostcity/app.ts",
    "dev": "concurrently \"npm start\" \"npm run server:watch\"",
    "web": "node --no-warnings --loader ts-node/esm src/lostcity/web.ts",
    "client:pack": "node --no-warnings --loader ts-node/esm src/lostcity/tools/client/all/pack.ts",
    "client:clean": "node --no-warnings --loader ts-node/esm src/lostcity/tools/client/clean.ts",
    "script:symbols": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/scripts/symbols.ts",
    "script:compile": "cd data && java -jar ../RuneScriptCompiler.jar src/scripts pack/server",
    "script:run": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/runscript.ts",
    "server:pack": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/all/pack.ts",
    "server:clean": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/clean.ts",
    "server:build": "npm run server:pack && npm run script:symbols && npm run script:compile",
    "server:watch": "watch \"npm run server:build\" ./data/src/scripts",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .js",
    "drops": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/drops.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/session": "^10.5.0",
    "@fastify/static": "^6.11.2",
    "@fastify/view": "^8.2.0",
    "@jimp/png": "^0.22.10",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.23.2",
    "jimp": "^0.22.10",
    "node-forge": "^1.3.1",
    "watch": "^1.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@swc/core": "1.3.81",
    "@swc/helpers": "^0.5.2",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/node-forge": "^1.3.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
