{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#jagex2/*": "./src/jagex2/*",
    "#lostcity/*": "./src/lostcity/*"
  },
  "scripts": {
    "start": "node --no-warnings --loader ts-node/esm src/lostcity/app.ts",
    "login": "node --no-warnings --loader ts-node/esm src/lostcity/login.ts",
    "friend": "node --no-warnings --loader ts-node/esm src/lostcity/friend.ts",
    "maintenance": "node --no-warnings --loader ts-node/esm src/lostcity/maintenance.ts",
    "web": "node --no-warnings --loader ts-node/esm src/lostcity/web.ts",
    "dev": "concurrently \"nodemon src/lostcity/app.ts\" \"npm run server:watch\"",
    "client:pack": "node --no-warnings --loader ts-node/esm src/lostcity/tools/client/all/pack.ts",
    "client:clean": "node --no-warnings --loader ts-node/esm src/lostcity/tools/client/clean.ts",
    "script:symbols": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/scripts/symbols.ts",
    "script:compile": "cd data && java -jar ../RuneScriptCompiler.jar src/scripts pack/server",
    "script:run": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/runscript.ts",
    "server:pack": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/all/pack.ts",
    "server:clean": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/clean.ts",
    "server:build": "npm run server:pack && npm run script:symbols && npm run script:compile",
    "server:watch": "watch \"npm run server:build\" ./data/src/scripts ./src/lostcity/tools",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .js",
    "drops": "node --no-warnings --loader ts-node/esm src/lostcity/tools/server/drops.ts",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:schema": "prisma migrate dev"
  },
  "prisma": {
    "seed": "node --no-warnings --loader ts-node/esm src/lostcity/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^6.12.0",
    "@fastify/view": "^8.2.0",
    "@jimp/png": "^0.22.10",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.25.2",
    "jimp": "^0.22.10",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.5",
    "node-forge": "^1.3.1",
    "watch": "^1.0.2",
    "ws": "^8.15.1"
  },
  "devDependencies": {
    "@2004scape/rsmod-pathfinder": "^4.2.5",
    "@swc/core": "1.3.101",
    "@swc/helpers": "^0.5.3",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-forge": "^1.3.10",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "prisma-kysely": "^1.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
