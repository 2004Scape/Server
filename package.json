{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#3rdparty/*": "./src/3rdparty/*",
    "#/*": "./src/*",
    "#tools/*": "./tools/*"
  },
  "scripts": {
    "build": "tsx tools/cache/pack.ts",
    "bundle": "npm run bundle:esbuild",
    "bundle:bun": "bun bundle.js",
    "bundle:esbuild": "tsx bundle.js",
    "clean": "tsx tools/cache/cleanServer.ts && tsx tools/cache/cleanClient.ts",
    "db:migrate": "prisma migrate deploy --schema prisma/multiworld/schema.prisma",
    "db:reset": "prisma migrate reset --force --schema prisma/multiworld/schema.prisma",
    "db:schema": "prisma migrate dev --schema prisma/multiworld/schema.prisma",
    "dev": "tsx watch --clear-screen=false --include \"src/*\" src/app.ts",
    "friend": "tsx src/friend.ts",
    "lint": "eslint --no-warn-ignored src tools",
    "lint:staged": "eslint --no-warn-ignored",
    "logger": "tsx src/logger.ts",
    "login": "tsx src/login.ts",
    "maintenance": "tsx src/appMaintenance.ts",
    "precommit": "prettier . --write && eslint src --fix",
    "prepare": "husky",
    "quickstart": "tsx src/app.ts",
    "setup": "tsx tools/server/setup.ts",
    "sqlite:migrate": "prisma migrate deploy --schema prisma/singleworld/schema.prisma",
    "sqlite:reset": "prisma migrate reset --force --schema prisma/singleworld/schema.prisma",
    "sqlite:schema": "prisma migrate dev --schema prisma/singleworld/schema.prisma",
    "start": "npm i && tsx src/app.ts",
    "test": "vitest run ./src",
    "todo": "leasot src/ --skip-unsupported --exit-nicely"
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "*.{js,ts}": [
      "npm run lint:staged",
      "leasot --skip-unsupported --exit-nicely"
    ]
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.3",
    "@jimp/js-png": "^1.6.0",
    "@jimp/plugin-quantize": "^1.6.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.8.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "jimp": "^1.6.0",
    "kleur": "^4.1.5",
    "kysely": "^0.27.5",
    "mysql2": "^3.12.0",
    "node-forge": "^1.3.1",
    "prom-client": "^15.1.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@2004scape/rsmod-pathfinder": "^5.0.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.1.14",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.10.2",
    "@types/node-forge": "^1.3.11",
    "@types/ws": "^8.5.13",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "leasot": "^14.4.0",
    "lint-staged": "^15.2.11",
    "prettier": "3.4.2",
    "prisma": "^6.1.0",
    "prisma-kysely": "^1.8.0",
    "sort-package-json": "^2.12.0",
    "tinybench": "^3.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8"
  },
  "prisma": {
    "seed": "tsx src/db/seed.ts"
  }
}
