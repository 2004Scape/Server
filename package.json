{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#jagex2/*": "./src/jagex2/*",
    "#lostcity/*": "./src/lostcity/*",
    "#rsmod/*": "./src/rsmod/*"
  },
  "scripts": {
    "start": "nodemon",
    "client:pack": "ts-node --esm src/lostcity/tools/client/pack/all.js",
    "client:clean": "ts-node --esm src/lostcity/tools/client/clean.ts",
    "server:pack": "ts-node --esm src/lostcity/tools/server/pack/all.js",
    "script:symbols": "ts-node --esm src/lostcity/tools/server/scripts/symbols.js",
    "script:compile": "cd data && java -jar ../RuneScriptCompiler.jar src/scripts pack/server",
    "script:run": "ts-node --esm src/lostcity/tools/server/runscript.ts",
    "server:clean": "ts-node --esm src/lostcity/tools/server/clean.ts",
    "server:build": "npm run server:pack && npm run script:symbols && npm run script:compile",
    "server:watch": "watch \"npm run server:build\" ./data/src/scripts",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .js"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.7.0",
    "@fastify/session": "^10.3.1",
    "@fastify/static": "^6.10.2",
    "@fastify/view": "^8.0.0",
    "@jimp/png": "^0.22.8",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.18.0",
    "jimp": "^0.22.8",
    "node-forge": "^1.3.1",
    "watch": "^1.0.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.3",
    "@types/node-forge": "^1.3.2",
    "@types/nodemon": "^1.19.2",
    "@types/watch": "^1.0.3",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
